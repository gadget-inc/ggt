{
  "name": "ggt",
  "version": "0.4.8",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": {
    "url": "https://github.com/gadget-inc/ggt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadget-inc/ggt.git"
  },
  "license": "MIT",
  "author": {
    "name": "Gadget Authors"
  },
  "type": "module",
  "main": "lib/main.js",
  "bin": {
    "ggt": "bin/run.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run clean && swc src -d lib",
    "changeset": "changeset",
    "clean": "rimraf lib tmp/spec",
    "dev": "vitest --watch",
    "generate:graphql": "graphql-codegen --config graphql-codegen.yml",
    "lint": "concurrently --group --names --prefix-colors=auto 'npm:lint:*(!fix)'",
    "lint:cspell": "cspell --no-progress --show-suggestions --show-context '**'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc --project tsconfig.json",
    "release": "npm run build && changeset publish",
    "test": "vitest",
    "version": "changeset version && npm install --package-lock-only && ./scripts/generate-readme.ts"
  },
  "dependencies": {
    "@sentry/node": "^7.92.0",
    "@swc/helpers": "^0.5.3",
    "arg": "^5.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "clean-stack": "^5.2.0",
    "cli-table3": "^0.6.3",
    "dayjs": "^1.11.10",
    "execa": "^8.0.1",
    "fast-levenshtein": "^3.0.0",
    "find-up": "^6.3.0",
    "fs-extra": "^11.2.0",
    "get-port": "^7.0.0",
    "got": "^13.0.0",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.3",
    "ignore": "^5.3.0",
    "is-wsl": "^3.1.0",
    "mimic-fn": "^5.0.0",
    "ms": "^2.1.3",
    "node-notifier": "^10.0.1",
    "normalize-package-data": "^6.0.0",
    "normalize-path": "^3.0.0",
    "open": "^9.1.0",
    "ora": "^7.0.1",
    "p-map": "^6.0.0",
    "p-queue": "^7.4.1",
    "p-retry": "^6.2.0",
    "p-timeout": "^6.1.2",
    "pluralize": "^8.0.0",
    "prompts": "^2.4.2",
    "semver": "^7.5.4",
    "serialize-error": "^11.0.3",
    "strip-ansi": "^7.1.0",
    "ts-dedent": "^2.2.0",
    "watcher": "^2.3.0",
    "which": "^4.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/eslint": "^8.56.1",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.19.5",
    "@types/node-notifier": "^8.0.5",
    "@types/normalize-path": "^3.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/prompts": "^2.4.9",
    "@types/which": "^3.0.3",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.1",
    "concurrently": "^8.2.2",
    "cspell": "^7.3.8",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^47.0.2",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "nock": "^13.4.0",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "rimraf": "^5.0.5",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
