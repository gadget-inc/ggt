{
  "name": "@gadgetinc/ggt",
  "version": "0.1.13",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": "gadget-inc/ggt",
  "license": "MIT",
  "author": "Gadget Authors",
  "main": "lib/index.js",
  "bin": {
    "ggt": "./bin/run"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "rimraf lib && tsc --build && oclif manifest",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "lint": "concurrently --prefix none --group 'npm:lint:*'",
    "lint:cspell": "cspell --no-progress --show-suggestions --show-context '**'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc --project tsconfig.json --project spec/tsconfig.json --noEmit",
    "publish": "npm run build && changeset publish",
    "readme": "patch-package && oclif readme && prettier --write README.md",
    "test": "jest",
    "version": "changeset version && npm install --package-lock-only && npm run build && npm run readme"
  },
  "dependencies": {
    "@oclif/core": "^2.8.1",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-not-found": "^2.3.21",
    "@oclif/plugin-warn-if-update-available": "^2.0.29",
    "@sentry/node": "^7.40.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "clean-stack": "^3.0.1",
    "date-fns": "^2.29.3",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^11.1.0",
    "get-port": "^5.1.1",
    "got": "^11.8.5",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.3",
    "ignore": "^5.2.4",
    "inquirer": "^8.2.5",
    "lodash": "^4.17.21",
    "node-notifier": "^10.0.1",
    "normalize-path": "^3.0.0",
    "open": "^8.4.1",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "pluralize": "^8.0.0",
    "serialize-error": "^8.1.0",
    "ts-dedent": "^2.2.0",
    "which": "^3.0.0",
    "ws": "^8.12.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@graphql-codegen/add": "^4.0.0",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@oclif/plugin-plugins": "^2.3.2",
    "@oclif/test": "^2.3.15",
    "@swc/core": "^1.3.35",
    "@swc/helpers": "^0.4.14",
    "@swc/jest": "^0.2.24",
    "@types/debug": "^4.1.7",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/node-notifier": "^8.0.2",
    "@types/normalize-path": "^3.0.0",
    "@types/pluralize": "^0.0.29",
    "@types/which": "^2.0.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "cspell": "^6.28.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-lodash": "^7.4.0",
    "jest": "^29.4.3",
    "jest-extended": "^3.2.3",
    "nock": "^13.3.0",
    "oclif": "^3.7.0",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.4",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "ggt",
    "dirname": "gadget",
    "commands": "./lib/commands",
    "helpClass": "./lib/utils/help",
    "topicSeparator": " ",
    "topics": {},
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  }
}
