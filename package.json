{
  "name": "@gadgetinc/ggt",
  "version": "0.1.4",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": "gadget-inc/ggt",
  "license": "MIT",
  "author": "Gadget Authors",
  "main": "lib/index.js",
  "bin": {
    "ggt": "./bin/run"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "rimraf lib && tsc --build",
    "postbuild": "patch-package && oclif manifest && oclif readme && prettier --write README.md",
    "lint": "concurrently --prefix none --group 'npm:lint:*'",
    "lint:cspell": "cspell --no-progress --show-suggestions --show-context '**'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc --project tsconfig.json --project spec/tsconfig.json --noEmit",
    "test": "jest",
    "version": "npm run build && git add README.md"
  },
  "dependencies": {
    "@oclif/core": "^1.16.4",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-not-found": "^2.3.3",
    "@oclif/plugin-warn-if-update-available": "^2.0.6",
    "@sentry/node": "^7.14.0",
    "chokidar": "^3.5.3",
    "clean-stack": "^3.0.1",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^10.1.0",
    "get-port": "^5.1.1",
    "got": "^11.8.5",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.11.2",
    "ignore": "^5.2.0",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "new-github-issue-url": "^0.2.1",
    "node-notifier": "^10.0.1",
    "normalize-path": "^3.0.0",
    "open": "^8.4.0",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "serialize-error": "^8.1.0",
    "ts-dedent": "^2.2.0",
    "which": "^2.0.2",
    "ws": "^8.8.1"
  },
  "devDependencies": {
    "@oclif/plugin-plugins": "^2.1.1",
    "@oclif/test": "^2.2.0",
    "@swc/core": "^1.3.3",
    "@swc/helpers": "^0.4.11",
    "@swc/jest": "^0.2.23",
    "@types/debug": "^4.1.7",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.4",
    "@types/jest": "^29.1.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.7.23",
    "@types/node-notifier": "^8.0.2",
    "@types/normalize-path": "^3.0.0",
    "@types/which": "^2.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "concurrently": "^7.4.0",
    "cspell": "^6.10.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-lodash": "^7.4.0",
    "jest": "^29.1.1",
    "jest-extended": "^3.1.0",
    "nock": "^13.2.9",
    "oclif": "^3.2.9",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "type-fest": "^3.0.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "ggt",
    "dirname": "gadget",
    "commands": "./lib/commands",
    "helpClass": "./lib/lib/help",
    "topicSeparator": " ",
    "topics": {},
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  }
}
