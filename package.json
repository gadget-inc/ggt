{
  "name": "ggt",
  "version": "2.1.0",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": {
    "url": "https://github.com/gadget-inc/ggt/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Gadget Authors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadget-inc/ggt.git"
  },
  "bin": {
    "ggt": "dist/main.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/package.json",
    "/README.md"
  ],
  "type": "module",
  "scripts": {
    "build": "pnpm run clean && esbuild src/main.ts --outdir=dist --format=esm --platform=node --target=node20 --inject:./shims/cjs.js --bundle --splitting --minify --keep-names --sourcemap",
    "build:watch": "pnpm run build --minify=false --sources-content=false --watch",
    "changeset": "changeset",
    "clean": "rimraf dist tmp/spec",
    "dev": "run-p -s 'test:watch --no-clear-screen' 'build:watch'",
    "generate:graphql": "graphql-codegen --config=graphql-codegen.yml",
    "lint": "pnpm run --aggregate-output '/^lint:(?!fix).*/'",
    "lint:cspell": "cspell . --no-progress --show-suggestions --show-context",
    "lint:fix": "oxfmt && oxlint --type-aware --fix",
    "lint:oxfmt": "oxfmt --check",
    "lint:oxlint": "oxlint --type-aware --deny-warnings",
    "release": "pnpm run build && changeset publish",
    "test": "vitest",
    "test:integration": "vitest --config spec/integration/vitest.integration.config.ts",
    "test:watch": "pnpm run test --standalone --watch",
    "version": "changeset version && oxfmt CHANGELOG.md && node --loader=ts-node/esm --no-warnings scripts/generate-readme.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@sentry/node": "^10.39.0",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.32",
    "@types/node-notifier": "^8.0.5",
    "@types/normalize-package-data": "^2.4.4",
    "@types/normalize-path": "^3.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/semver": "^7.7.1",
    "@types/set-cookie-parser": "^2.4.10",
    "@types/tcp-port-used": "^1.0.0",
    "@types/which": "^3.0.4",
    "@types/ws": "^8.18.1",
    "ansi-escapes": "^7.2.0",
    "arg": "^5.0.2",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "chalk-template": "^1.1.2",
    "clean-stack": "^6.0.0",
    "cli-cursor": "^5.0.0",
    "cli-spinners": "^3.4.0",
    "cli-table3": "^0.6.5",
    "cspell": "^9.6.4",
    "dayjs": "^1.11.19",
    "esbuild": "^0.27.3",
    "execa": "^9.6.0",
    "fast-levenshtein": "^3.0.0",
    "fast-querystring": "^1.1.2",
    "figures": "^6.1.0",
    "find-up": "^8.0.0",
    "fs-extra": "^11.3.3",
    "get-port": "^7.1.0",
    "got": "^14.6.6",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.7",
    "ignore": "^7.0.5",
    "indent-string": "^5.0.0",
    "is-interactive": "^2.0.0",
    "is-unicode-supported": "^2.1.0",
    "is-wsl": "^3.1.0",
    "mimic-function": "^5.0.1",
    "ms": "^2.1.3",
    "nock": "^14.0.11",
    "node-notifier": "^10.0.1",
    "normalize-package-data": "^8.0.0",
    "normalize-path": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^11.0.0",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.14.1",
    "p-map": "^7.0.3",
    "p-queue": "^9.0.0",
    "p-retry": "^7.1.0",
    "p-timeout": "^7.0.1",
    "pluralize": "^8.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "rimraf": "^6.1.0",
    "semver": "^7.7.3",
    "serialize-error": "^13.0.1",
    "set-cookie-parser": "^3.0.1",
    "simple-git": "^3.30.0",
    "string-width": "^8.2.0",
    "strip-ansi": "^7.1.2",
    "tcp-port-used": "^1.0.2",
    "terminal-link": "^5.0.0",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.2",
    "type-fest": "^5.4.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "watcher": "^2.3.1",
    "which": "^6.0.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.11.0"
}
