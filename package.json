{
  "name": "@gadgetinc/ggt",
  "version": "0.0.0",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": "gadget-inc/ggt",
  "license": "MIT",
  "author": "Gadget Authors",
  "main": "dist/index.js",
  "bin": {
    "ggt": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif readme",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --quiet --ext ts,tsx .",
    "lint:eslint:fix": "eslint --quiet --ext ts,tsx --fix .",
    "lint:fix": "yarn run lint:prettier:fix && yarn run lint:eslint:fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --write --check .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "typegen": "shx rm src/__generated__/*.typegen.ts && xstate typegen src/**/*.ts && shx mv src/**/*.typegen.ts src/__generated__"
  },
  "dependencies": {
    "@oclif/core": "1.11.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5.1.12",
    "chokidar": "^3.5.3",
    "env-paths": "^2.0.0",
    "fs-extra": "^9.1.0",
    "get-port": "^5.1.1",
    "got": "^11.8.5",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.5.0",
    "ignore": "^5.2.0",
    "inquirer": "^8.2.3",
    "lodash": "^4.17.21",
    "open": "^8.4.0",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "serialize-error": "^7.0.1",
    "ws": "^8.8.0",
    "xstate": "^4.29.0"
  },
  "devDependencies": {
    "@oclif/plugin-plugins": "^2.1.0",
    "@oclif/test": "^2.1.0",
    "@swc/core": "1.2.186",
    "@swc/jest": "^0.2.21",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^17.0.42",
    "@types/pino-pretty": "^4.7.5",
    "@xstate/cli": "^0.2.1",
    "jest": "^27.5.1",
    "jest-extended": "^3.0.1",
    "nock": "^13.2.2",
    "oclif": "^3.1.2",
    "prettier": "^2.4.1",
    "shx": "^0.3.4",
    "tslib": "^2.3.1",
    "type-fest": "^2.17.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "bin": "ggt",
    "dirname": "gadget",
    "commands": "./dist/commands",
    "repositoryPrefix": "gadget-inc/ggt",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  }
}
