{
  "name": "ggt",
  "version": "1.7.4",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": {
    "url": "https://github.com/gadget-inc/ggt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadget-inc/ggt.git"
  },
  "license": "MIT",
  "author": {
    "name": "Gadget Authors"
  },
  "type": "module",
  "bin": {
    "ggt": "dist/main.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "pnpm run clean && esbuild src/main.ts --outdir=dist --format=esm --platform=node --target=node20 --inject:./shims/cjs.js --bundle --splitting --minify --keep-names --sourcemap",
    "build:watch": "pnpm run build --minify=false --sources-content=false --watch",
    "changeset": "changeset",
    "clean": "rimraf dist tmp/spec",
    "dev": "run-p -s 'test:watch --no-clear-screen' 'build:watch'",
    "generate:graphql": "graphql-codegen --config=graphql-codegen.yml",
    "lint": "run-p 'lint:!(fix)' --aggregate-output --print-label --continue-on-error",
    "lint:cspell": "cspell . --no-progress --show-suggestions --show-context",
    "lint:eslint": "eslint . --max-warnings=0 --cache --cache-location=node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "prettier . --write --cache && eslint . --fix --cache --cache-location=node_modules/.cache/eslint/.eslintcache",
    "lint:prettier": "prettier . --check --cache",
    "lint:typescript": "tsc --project tsconfig.json",
    "release": "pnpm run build && changeset publish",
    "test": "vitest --cache",
    "test:watch": "pnpm run test --watch",
    "version": "changeset version && prettier --write CHANGELOG.md && node --loader=ts-node/esm --no-warnings scripts/generate-readme.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@sentry/node": "^9.22.0",
    "@types/eslint": "^8.56.7",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.24",
    "@types/node-notifier": "^8.0.5",
    "@types/normalize-package-data": "^2.4.4",
    "@types/normalize-path": "^3.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/semver": "^7.7.1",
    "@types/set-cookie-parser": "^2.4.10",
    "@types/which": "^3.0.4",
    "@types/ws": "^8.5.14",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "ansi-escapes": "^7.0.0",
    "arg": "^5.0.2",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "chalk-template": "^1.1.2",
    "clean-stack": "^5.2.0",
    "cli-cursor": "^5.0.0",
    "cli-spinners": "^2.9.2",
    "cli-table3": "^0.6.5",
    "cspell": "^9.3.0",
    "dayjs": "^1.11.13",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "execa": "^8.0.1",
    "fast-levenshtein": "^3.0.0",
    "fast-querystring": "^1.1.2",
    "figures": "^6.1.0",
    "find-up": "^7.0.0",
    "fs-extra": "^11.3.0",
    "get-port": "^7.1.0",
    "got": "^13.0.0",
    "graphql": "^16.12.0",
    "graphql-ws": "^5.16.2",
    "ignore": "^6.0.2",
    "indent-string": "^5.0.0",
    "is-interactive": "^2.0.0",
    "is-unicode-supported": "^2.1.0",
    "is-wsl": "^3.1.0",
    "mimic-function": "^5.0.1",
    "ms": "^2.1.3",
    "nock": "^14.0.0",
    "node-notifier": "^10.0.1",
    "normalize-package-data": "^7.0.0",
    "normalize-path": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "open": "^10.1.2",
    "p-map": "^7.0.3",
    "p-queue": "^9.0.0",
    "p-retry": "^6.2.1",
    "p-timeout": "^6.1.4",
    "pluralize": "^8.0.0",
    "prettier": "^3.5.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.14",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-toc": "^9.0.0",
    "rimraf": "^6.1.0",
    "semver": "^7.7.3",
    "serialize-error": "^11.0.3",
    "set-cookie-parser": "^2.7.1",
    "simple-git": "^3.30.0",
    "stdin-discarder": "^0.2.2",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.0",
    "terminal-link": "^4.0.0",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.7.3",
    "vitest": "^3.1.4",
    "watcher": "^2.3.1",
    "which": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.4"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
