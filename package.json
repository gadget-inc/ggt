{
  "name": "ggt",
  "version": "1.0.4",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": {
    "url": "https://github.com/gadget-inc/ggt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadget-inc/ggt.git"
  },
  "license": "MIT",
  "author": {
    "name": "Gadget Authors"
  },
  "type": "module",
  "main": "lib/ggt.js",
  "bin": {
    "ggt": "lib/main.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run clean && swc src --out-dir=lib --strip-leading-paths",
    "changeset": "changeset",
    "clean": "rimraf lib tmp/spec",
    "dev": "vitest --watch",
    "generate:graphql": "graphql-codegen --config=graphql-codegen.yml",
    "lint": "concurrently 'npm:lint:*(!fix)' --group --names --prefix-colors=auto",
    "lint:cspell": "cspell . --no-progress --show-suggestions --show-context",
    "lint:eslint": "eslint . --max-warnings=0 --cache --cache-location=node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "eslint . --fix --cache && prettier . --write --cache",
    "lint:prettier": "prettier . --check --cache",
    "lint:typescript": "tsc --project tsconfig.json",
    "release": "npm run build && changeset publish",
    "test": "vitest",
    "version": "changeset version && npm install --package-lock-only && scripts/generate-readme.ts"
  },
  "dependencies": {
    "@sentry/node": "^8.17.0",
    "@swc/helpers": "^0.5.12",
    "ansi-escapes": "^7.0.0",
    "arg": "^5.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "clean-stack": "^5.2.0",
    "cli-cursor": "^4.0.0",
    "cli-spinners": "^2.9.2",
    "cli-table3": "^0.6.5",
    "dayjs": "^1.11.11",
    "execa": "^9.3.0",
    "fast-levenshtein": "^3.0.0",
    "figures": "^6.1.0",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "get-port": "^7.1.0",
    "got": "^13.0.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "ignore": "^5.3.1",
    "indent-string": "^5.0.0",
    "is-interactive": "^2.0.0",
    "is-unicode-supported": "^2.0.0",
    "is-wsl": "^3.1.0",
    "mimic-function": "^5.0.1",
    "ms": "^2.1.3",
    "node-notifier": "^10.0.1",
    "normalize-package-data": "^6.0.2",
    "normalize-path": "^3.0.0",
    "open": "^10.1.0",
    "p-map": "^7.0.2",
    "p-queue": "^8.0.1",
    "p-retry": "^6.2.0",
    "p-timeout": "^6.1.2",
    "pluralize": "^8.0.0",
    "semver": "^7.6.2",
    "serialize-error": "^11.0.3",
    "simple-git": "^3.25.0",
    "stdin-discarder": "^0.2.2",
    "string-width": "^7.2.0",
    "strip-ansi": "^7.1.0",
    "terminal-link": "^3.0.0",
    "ts-dedent": "^2.2.0",
    "watcher": "^2.3.1",
    "which": "^4.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@swc-node/register": "1.9.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.6.13",
    "@types/eslint": "^8.56.7",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.19.39",
    "@types/node-notifier": "^8.0.5",
    "@types/normalize-path": "^3.0.2",
    "@types/pluralize": "^0.0.33",
    "@types/which": "^3.0.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "concurrently": "^8.2.2",
    "cspell": "^8.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.5.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^54.0.0",
    "nock": "^13.5.4",
    "prettier": "^3.3.2",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "rimraf": "^5.0.7",
    "type-fest": "^4.20.1",
    "typescript": "^5.5.3",
    "vitest": "^2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
