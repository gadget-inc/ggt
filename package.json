{
  "name": "@gadgetinc/ggt",
  "version": "0.0.0",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": "gadget-inc/ggt",
  "license": "MIT",
  "author": "Gadget Authors",
  "main": "dist/index.js",
  "bin": {
    "ggt": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && oclif readme",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --quiet .",
    "lint:eslint:fix": "eslint --quiet --fix .",
    "lint:fix": "npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check .",
    "lint:prettier:fix": "prettier --check --write .",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "typegen": "shx rm src/__generated__/*.typegen.ts && xstate typegen src/**/*.ts && shx mv src/**/*.typegen.ts src/__generated__"
  },
  "dependencies": {
    "@oclif/core": "1.11.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/plugin-help": "^5.1.12",
    "chokidar": "^3.5.3",
    "env-paths": "^2.2.1",
    "fs-extra": "^10.1.0",
    "get-port": "^5.1.1",
    "got": "^11.8.5",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.9.1",
    "ignore": "^5.2.0",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "open": "^8.4.0",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "pino": "^8.3.1",
    "pino-pretty": "^8.1.0",
    "serialize-error": "^8.1.0",
    "ws": "^8.8.1",
    "xstate": "^4.32.1"
  },
  "devDependencies": {
    "@gadgetinc/prettier-config": "*",
    "@oclif/plugin-plugins": "^2.1.0",
    "@oclif/test": "^2.1.0",
    "@swc/core": "^1.2.223",
    "@swc/helpers": "^0.4.3",
    "@swc/jest": "^0.2.22",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.3",
    "@types/pino-pretty": "^4.7.5",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@xstate/cli": "^0.3.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-lodash": "^7.4.0",
    "jest": "^28.1.3",
    "jest-extended": "^3.0.1",
    "nock": "^13.2.9",
    "oclif": "^3.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "type-fest": "^2.18.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "ggt",
    "dirname": "gadget",
    "commands": "./dist/commands",
    "repositoryPrefix": "gadget-inc/ggt",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  }
}
