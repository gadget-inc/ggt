{
  "name": "ggt",
  "version": "0.3.2",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gadget-inc/ggt.git"
  },
  "license": "MIT",
  "author": "Gadget Authors",
  "type": "module",
  "main": "lib/main.js",
  "bin": {
    "ggt": "bin/run.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run clean && swc src -d lib",
    "clean": "rimraf lib tmp",
    "generate:graphql": "graphql-codegen --config graphql-codegen.yml",
    "generate:readme": "FORCE_COLOR=0 node --loader @swc-node/register/esm --no-warnings ./scripts/generate-readme.ts",
    "lint": "concurrently --prefix none --group 'npm:lint:*(!fix)'",
    "lint:cspell": "cspell --no-progress --show-suggestions --show-context '**'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc --project tsconfig.json",
    "release": "npm run build && changeset publish",
    "test": "cross-env NODE_OPTIONS=\"--loader @swc-node/register/esm --no-warnings\" vitest",
    "version": "changeset version && npm install --package-lock-only && npm run generate:readme"
  },
  "dependencies": {
    "@sentry/node": "^7.75.0",
    "@swc/helpers": "^0.5.3",
    "arg": "^5.0.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "clean-stack": "^5.2.0",
    "date-fns": "^2.30.0",
    "debug": "^4.3.4",
    "execa": "^8.0.1",
    "fast-levenshtein": "^3.0.0",
    "find-up": "^6.3.0",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "got": "^13.0.0",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.2",
    "ignore": "^5.2.4",
    "inquirer": "^9.2.11",
    "is-wsl": "^3.1.0",
    "ms": "^2.1.3",
    "node-notifier": "^10.0.1",
    "normalize-package-data": "^6.0.0",
    "normalize-path": "^3.0.0",
    "open": "^9.1.0",
    "p-map": "^6.0.0",
    "p-queue": "^7.4.1",
    "p-retry": "^6.1.0",
    "pluralize": "^8.0.0",
    "semver": "^7.5.4",
    "serialize-error": "^11.0.2",
    "ts-dedent": "^2.2.0",
    "watcher": "^2.3.0",
    "which": "^4.0.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.95",
    "@types/debug": "^4.1.10",
    "@types/eslint": "^8.44.6",
    "@types/fast-levenshtein": "^0.0.3",
    "@types/fs-extra": "^11.0.3",
    "@types/inquirer": "^9.0.6",
    "@types/ms": "^0.7.33",
    "@types/node": "^20.8.8",
    "@types/node-notifier": "^8.0.4",
    "@types/normalize-path": "^3.0.1",
    "@types/pluralize": "^0.0.32",
    "@types/which": "^3.0.1",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cspell": "^7.3.8",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-unicorn": "^48.0.1",
    "nock": "^13.3.6",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "rimraf": "^5.0.5",
    "type-fest": "^4.5.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
