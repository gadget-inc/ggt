{
  "name": "ggt",
  "version": "0.2.3",
  "description": "The command-line interface for Gadget",
  "homepage": "https://github.com/gadget-inc/ggt",
  "bugs": "https://github.com/gadget-inc/ggt/issues",
  "repository": "gadget-inc/ggt",
  "license": "MIT",
  "author": "Gadget Authors",
  "type": "module",
  "main": "lib/index.js",
  "bin": {
    "ggt": "./bin/run.js"
  },
  "files": [
    "/assets",
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/package.json",
    "/README.md"
  ],
  "scripts": {
    "build": "npm run clean && swc src -d lib && oclif manifest",
    "clean": "rimraf lib tmp oclif.manifest.json",
    "generate-graphql": "graphql-codegen --config graphql-codegen.yml",
    "lint": "concurrently --prefix none --group 'npm:lint:*(!fix)'",
    "lint:cspell": "cspell --no-progress --show-suggestions --show-context '**'",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc --project tsconfig.json",
    "publish": "npm run build && changeset publish",
    "readme": "patch-package && oclif readme && prettier --write README.md",
    "test": "cross-env NODE_OPTIONS=\"--no-warnings --loader ./node_modules/ts-node/esm.mjs\" vitest",
    "version": "changeset version && npm install --package-lock-only && npm run build && npm run readme"
  },
  "dependencies": {
    "@oclif/core": "^2.9.4",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-not-found": "^2.4.0",
    "@oclif/plugin-warn-if-update-available": "^2.0.45",
    "@sentry/node": "^7.66.0",
    "@swc/helpers": "^0.5.1",
    "chalk": "^5.3.0",
    "chalk-template": "^1.1.0",
    "clean-stack": "^5.2.0",
    "date-fns": "^2.30.0",
    "debug": "^4.3.4",
    "execa": "^8.0.1",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "got": "^13.0.0",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.14.0",
    "ignore": "^5.2.4",
    "inquirer": "^9.2.10",
    "lodash": "^4.17.21",
    "node-notifier": "^10.0.1",
    "normalize-path": "^3.0.0",
    "open": "^9.1.0",
    "p-map": "^6.0.0",
    "p-queue": "^7.4.0",
    "pluralize": "^8.0.0",
    "serialize-error": "^11.0.2",
    "ts-dedent": "^2.2.0",
    "watcher": "^2.3.0",
    "which": "^4.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@oclif/plugin-plugins": "^3.1.8",
    "@oclif/test": "^2.5.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.81",
    "@types/debug": "^4.1.8",
    "@types/eslint": "^8.44.2",
    "@types/fast-levenshtein": "^0.0.2",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.7",
    "@types/node-notifier": "^8.0.2",
    "@types/normalize-path": "^3.0.0",
    "@types/pluralize": "^0.0.30",
    "@types/which": "^3.0.0",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "cspell": "^7.2.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-lodash": "^7.4.0",
    "nock": "^13.3.3",
    "oclif": "^3.13.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-packagejson": "^2.4.5",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "type-fest": "^4.3.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "oclif": {
    "bin": "ggt",
    "dirname": "gadget",
    "commands": "./lib/commands",
    "helpClass": "./lib/services/help",
    "topicSeparator": " ",
    "topics": {},
    "plugins": [
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 1
    }
  }
}
